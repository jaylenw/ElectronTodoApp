{
  "name": "electrontodoapp",
  "productName": "Electrontodoapp",
  "version": "0.0.0",
  "description": "My good app",
  "license": "MIT",
  "repository": "jaylenw/electrontodoapp",
  "author": {
    "name": "",
    "email": "",
    "url": "github.com/jaylenw"
  },
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "build": "electron-packager . --out=dist --overwrite --all",
    "deb64": "electron-installer-debian --src dist/Electrontodoapp-linux-x64/ --dest dist/installers/ --arch amd64 --bin '/usr/lib/electrontodoapp/Electrontodoapp'",
    "rpm64": "electron-installer-redhat --src dist/Electrontodoapp-linux-x64/ --dest dist/installers/ --arch x86_64",
    "flatpak64": "electron-installer-flatpak --src dist/Electrontodoapp-linux-x64/ --dest dist/installers/ --arch x64",
    "dmg": "electron-installer-dmg ./dist/Electrontodoapp-mas-x64/ Electrontodoapp-mas-x64 --out=./dist/installers/"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "angular": "~1.6.1",
    "electron-debug": "~1.0.0",
    "ng-showdown": "~1.1.0"
  },
  "devDependencies": {
    "devtron": "~1.1.0",
    "electron-packager": "~8.0.0",
    "electron": "~1.3.3",
    "electron-installer-debian": "~0.5.2",
    "electron-installer-dmg": "~0.2.1",
    "electron-installer-flatpak": "~0.6.0",
    "electron-installer-redhat": "~0.5.0",
    "electron-packager": "~8.0.0",
    "xo": "~0.16.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
